{
  "name": "orms-web",
  "description": "",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/equdev/ORMSWeb"
  },
  "license": "MIT",
  "engines": {
    "node": ">=14.x"
  },
  "keywords": [
    "react",
    "create-react-app",
    "template",
    "typescript",
    "redux",
    "reduxjs",
    "react-redux",
    "react-boilerplate"
  ],
  "scripts": {
    "// ---DEFAULT CRA---": "",
    "start": "react-app-rewired start",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test",
    "// ---TEMPLATE---": "Scripts will be also in the template",
    "start:prod": "yarn run build && serve -s build",
    "checkTs": "tsc --noEmit",
    "eslint": "eslint --ext js,ts,tsx",
    "lint": "yarn run eslint src",
    "lint:fix": "yarn run eslint --fix src",
    "lint:css": "stylelint src/**/*.css",
    "generate": "plop --plopfile internals/generators/plopfile.ts",
    "cleanAndSetup": "ts-node ./internals/scripts/clean.ts",
    "prettify": "prettier --write",
    "// ---TESTING TEMPLATE---": "",
    "verify-startingTemplate-changes": "ts-node ./internals/scripts/verify-startingTemplate-changes.ts",
    "test:cov": "yarn run test --watchAll=false -- --coverage"
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": [
      "yarn run eslint --fix"
    ],
    "*.{md,json}": [
      "prettier --write"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*/*.d.ts",
      "!src/**/*/Loadable.ts",
      "!src/**/*/messages.ts",
      "!src/reportWebVitals.ts",
      "!src/**/*/types.ts",
      "!src/index.tsx"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    },
    "moduleNameMapper": {
      "^jest.setup": "<rootDir>/jest.setup",
      "axios": "axios/dist/node/axios.cjs"
    },
    "modulePathIgnorePatterns": [
      "__fixtures__"
    ]
  },
  "dependencies": {
    "@emotion/react": "11.10.5",
    "@emotion/styled": "11.10.5",
    "@mui/material": "5.10.14",
    "@reduxjs/toolkit": "1.9.0",
    "axios": "1.1.3",
    "fontfaceobserver": "2.3.0",
    "process": "0.11.10",
    "react": "18.2.0",
    "react-app-polyfill": "3.0.0",
    "react-app-rewired": "2.2.1",
    "react-dom": "18.2.0",
    "react-helmet-async": "1.3.0",
    "react-hook-form": "7.39.4",
    "react-i18next": "12.0.0",
    "react-is": "18.2.0",
    "react-redux": "8.0.5",
    "react-router-dom": "6.4.3",
    "react-scripts": "5.0.1",
    "redux-injectors": "2.0.0",
    "redux-saga": "1.2.1",
    "sanitize.css": "13.0.0",
    "web-vitals": "3.0.4"
  },
  "devDependencies": {
    "@commitlint/cli": "17.2.0",
    "@commitlint/config-conventional": "17.2.0",
    "@testing-library/jest-dom": "5.16.5",
    "@testing-library/react": "13.4.0",
    "@types/fontfaceobserver": "^2.1.0",
    "@types/jest": "29.2.3",
    "@types/lodash": "4.14.189",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "@types/react-redux": "^7.1.24",
    "@types/react-test-renderer": "^18.0.0",
    "@types/rimraf": "^3.0.2",
    "@types/shelljs": "^0.8.11",
    "@types/testing-library__jest-dom": "^5.14.5",
    "@types/webpack": "^5.28.0",
    "@types/webpack-env": "^1.18.0",
    "chalk": "5.1.2",
    "cross-env": "7.0.3",
    "devmoji": "2.3.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-prettier": "4.2.1",
    "eslint-plugin-react-hooks": "4.6.0",
    "gh-pages": "4.0.0",
    "git-branch-is": "4.0.0",
    "husky": "8.0.2",
    "i18next-scanner": "4.1.0",
    "inquirer": "8.2.4",
    "inquirer-directory": "2.2.0",
    "lint-staged": "13.0.3",
    "node-plop": "0.31.0",
    "plop": "2.7.6",
    "prettier": "2.7.1",
    "react-test-renderer": "^16.13.1",
    "replace-in-file": "6.3.5",
    "rimraf": "3.0.2",
    "sass": "1.56.1",
    "serve": "14.1.1",
    "shelljs": "0.8.5",
    "standard-version": "9.5.0",
    "stylelint": "14.15.0",
    "stylelint-config-recommended": "9.0.0",
    "ts-jest": "29.0.3",
    "ts-node": "10.9.1",
    "typescript": "4.8.4"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
